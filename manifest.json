{
    "manifest_version": 2,
    "name": "Wireframe Toggle",
    "description":"Creating a toggle for checking of each elements and layout issues",
    "version":"1.0.0",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon":"icon.png",
        "default_popup":"toggle.html"
    },
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ]
}